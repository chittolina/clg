{
  "name": "fsts",
  "version": "1.0.0",
  "description": "Full Stack TypeScript",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "lint:api": "tsc --noEmit --p src/api && eslint src/api/**/*.ts --fix",
    "lint:client": "tsc --noEmit --p src/client --jsx react && eslint src/client/**/*.tsx --fix",
    "build:api": "tsc --p src/api --outDir dist/api",
    "build:client": "rm -rf dist/client && parcel build src/client/index.html -d dist/client",
    "start:api": "node dist/api/index.js",
    "dev:api": "yarn build:api && yarn start:api",
    "dev:client": "parcel src/client/index.html -d dist/client",
    "test": "NODE_ENV=test ts-node tests/index.ts",
    "build": "yarn build:api && yarn build:client",
    "start": "yarn start:api"
  },
  "dependencies": {
    "@types/node-schedule": "^1.2.4",
    "axios": "^0.19.0",
    "knex": "^0.19.5",
    "koa": "^2.8.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mongoose": "^5.7.4",
    "node-schedule": "^1.3.2",
    "path": "^0.12.7",
    "ramda": "^0.26.1",
    "react": "^16.0.0",
    "react-dom": "^16.10.2",
    "semantic-ui-react": "^0.88.1",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/koa": "^2.0.50",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.20",
    "@types/ramda": "^0.26.29",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/sinon": "^7.5.0",
    "@types/sqlite3": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "axios-mock-adapter": "^1.17.0",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "mocha": "^6.2.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
